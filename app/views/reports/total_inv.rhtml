<h1>Инвентаризация</h1>

<div align="center">
	<TABLE border="1" width="80%">
		<TR>
			<TD width="10%">Стойка</TD>
			<TD width="10%">Производитель</TD>
			<TD width="35%">Название</TD>
			<TD width="15%">P/N</TD>
			<TD width="15%">S/N</TD>
			<TD width="15%">L/N</TD>
		</TR>
		<% @devs.each do |dev| %>
		<TR>
			<TD><%= dev.location %></TD>
			<TD><%= dev.vendor.name %></TD>
			<TD><%= dev.labnumber %></TD>
			<TD><%= dev.partnumber %></TD>
			<TD><%= dev.serialnumber %></TD>
		</TR>
		<% end %>
	</TABLE>
</div>
